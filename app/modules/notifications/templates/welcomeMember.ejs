      @import url('https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,400;0,500;0,600;0,700;0,800;0,900;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');
      @import url('https://fonts.googleapis.com/css2?family=Roboto&display=swap');
      body {
        font-family: Raleway;
      }
      a {
        text-decoration: none;
      }
      #templateColumns {
        margin: 0 auto;
      }
      .topCategotyContainer {
        width: 100%;
      }
      .spanCategory {
        font-weight: 600;
        font-size: 16px;
        color: #252525;
        margin: 0 20px;
        line-height: 25px;
      }
      .separator {
        height: 1px;
        background-color: grey;
        margin: 15px 0;
      }

      .categoryImg {
        width: 100%;
        height: auto !important;
        object-fit: contain !important;
      }

      .categoryInfo {
        padding-left: 10px;
      }
      .categoryTitle {
        font-size: 20px; 
        color: #252525;
        font-weight: 600;
        line-height: 20px;
        margin: 0;
        text-align: start;
      }
      .categoryDes {
        font-size: 16px; 
        color: #252525;
        line-height: 20px;
        margin: 0;
        text-align: start;
      }

      .leanMoreButton {
        padding: 15px;
        margin: 15px 0  30px;
        background-color:#FEDD44;
        font-weight: 700;
        font-size: 12px;
        color: #252525;
        text-transform: uppercase;
        width: fit-content;
      }
      @media only screen and (max-width: 480px) {
        #templateColumns {
          width: 100% !important;
        }

        .templateColumnContainer {
          display: block !important;
          width: 100% !important;
        }

        .columnImage {
          height: auto !important;
          max-width: 480px !important;
          width: 100% !important;
        }

        .logoImage {
          height: auto !important;
          max-width: 200px !important;
          width: 100% !important;
        }

        .leftColumnContent {
          font-size: 16px !important;
          line-height: 125% !important;
          text-align: center;
        }

        .rightColumnContent {
          font-size: 16px !important;
          line-height: 125% !important;
          text-align: center;
        }

